<p id="notice"><%= notice %></p>

<h1>Telefonos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre telefono</th>
      <th>Ps telefono</th>
      <th>Oc telefono</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @telefonos.each do |telefono| %>
      <tr>
        <td><%= telefono.nombre_telefono %></td>
        <td><%= telefono.ps_telefono %></td>
        <td><%= telefono.oc_telefono %></td>
        <td><%= link_to 'Show', telefono %></td>
        <td><%= link_to 'Edit', edit_telefono_path(telefono) %></td>
        <td><%= link_to 'Destroy', telefono, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Telefono', new_telefono_path %>
