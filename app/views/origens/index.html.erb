<p id="notice"><%= notice %></p>

<h1>Origens</h1>

<table>
  <thead>
    <tr>
      <th>Nombre origen</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @origens.each do |origen| %>
      <tr>
        <td><%= origen.nombre_origen %></td>
        <td><%= origen.descripcion %></td>
        <td><%= link_to 'Show', origen %></td>
        <td><%= link_to 'Edit', edit_origen_path(origen) %></td>
        <td><%= link_to 'Destroy', origen, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Origen', new_origen_path %>
