<p id="notice"><%= notice %></p>

<h1>Internets</h1>

<table>
  <thead>
    <tr>
      <th>Pack</th>
      <th>Tipo pago</th>
      <th>Costo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @internets.each do |internet| %>
      <tr>
        <td><%= internet.pack %></td>
        <td><%= internet.tipo_pago %></td>
        <td><%= internet.costo %></td>
        <td><%= link_to 'Show', internet %></td>
        <td><%= link_to 'Edit', edit_internet_path(internet) %></td>
        <td><%= link_to 'Destroy', internet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Internet', new_internet_path %>
